{
  "name": "clinic-reservation-system",
  "version": "1.0.0",
  "description": "Serverless clinic reservation system",
  "main": "index.js",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "logs": "serverless logs -f",
    "invoke": "serverless invoke -f",
    "offline": "serverless offline",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "build": "tsc",
    "migrate": "node database/migrate.js",
    "migrate:dev": "NODE_ENV=development node database/migrate.js",
    "migrate:prod": "NODE_ENV=production node database/migrate.js",
    "setup": "npm install && npm run build",
    "dev": "serverless offline --stage dev",
    "validate": "npm run lint && npm run test && npm run build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup:local": "node scripts/setup-local.js",
    "start:local": "npm run docker:up && sleep 5 && npm run setup:local && npm run dev",
    "typecheck": "tsc --noEmit",
    "seed": "node scripts/seed-data.js",
    "clean:local": "node scripts/clean-local.js",
    "test:api": "node scripts/test-api.js"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.454.0",
    "@aws-sdk/client-rds-data": "^3.454.0",
    "@aws-sdk/client-secrets-manager": "^3.454.0",
    "@aws-sdk/client-ses": "^3.454.0",
    "@aws-sdk/client-sns": "^3.454.0",
    "@aws-sdk/client-sqs": "^3.454.0",
    "@aws-sdk/lib-dynamodb": "^3.454.0",
    "@line/bot-sdk": "^8.0.0",
    "agora-access-token": "^2.0.4",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1490.0",
    "bcryptjs": "^2.4.3",
    "i18next": "^23.7.6",
    "i18next-node-fs-backend": "^2.1.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "moment-timezone": "^0.5.43",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "stripe": "^14.5.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.126",
    "@types/bcryptjs": "^2.4.6",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.0",
    "serverless-webpack": "^5.13.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "clinic",
    "reservation"
  ],
  "author": "",
  "license": "MIT"
}
